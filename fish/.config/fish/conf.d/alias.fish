# Set up command aliases

# https://github.com/ogham/exa
alias ls "exa"
alias tree "exa --tree"

# https://github.com/sharkdp/bat
alias cat "bat"

alias gc "git commit -m"
alias gca "git commit -am"
alias gpush "git push"
alias gpull "git pull"
alias gs "git status"
alias gm "git merge"
alias gr "git rebase"
alias gclean "git clean -f"
